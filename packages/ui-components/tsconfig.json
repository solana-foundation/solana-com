{
  "extends": "../tsconfig/base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noUncheckedIndexedAccess": false,
    "moduleResolution": "node",
    "noEmit": false
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}