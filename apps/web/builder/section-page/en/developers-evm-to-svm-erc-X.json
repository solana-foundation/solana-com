          {
            "@type": "@builder.io/sdk:Element",
            "@version": 2,
            "id": "builder-ad171d39c86043308adbbbd7da63ec7a",
            "meta": {
              "previousId": "builder-9b8e5b86b6034888bb44b2bd1b174771"
            },
            "component": {
              "name": "Code Block",
              "options": {
                "code": "import { Connection, PublicKey } from \"@solana/web3.js\";\nimport { getAccount, getAssociatedTokenAddressSync } from \"@solana/spl-token\";\n\nconst connection = new Connection(\"https://api.testnet.solana.com\");\nconst ownerAddress = new PublicKey(\"Your Wallet Address\");\nconst mintAddress = new PublicKey(\"Token Address\");\n\nasync function allowance_orginal(_owner /*spender is not needed*/ ){\n    const ownerTokenAddress = getAssociatedTokenAddressSync(mintAddress, _owner)\n    const accountInfo = await getAccount(connection, ownerTokenAddress);\n\n    return accountInfo.delegatedAmount;\n}\n\nallowance_orginal(ownerAddress).then(result => {\n  console.log(result);\n}).catch(error => {\n  console.error(\"Error:\", error);\n});",
                "language": "jsx"
              }
            },
            "responsiveStyles": {
              "large": {
