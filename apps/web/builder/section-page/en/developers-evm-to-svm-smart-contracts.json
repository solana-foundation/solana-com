          },
          {
            "@type": "@builder.io/sdk:Element",
            "@version": 2,
            "id": "builder-fd22f99fb29a4f0faf4b88b36bc598d0",
            "meta": {
              "previousId": "builder-58c5660b31674590bb32575e4eaa945f"
            },
            "component": {
              "name": "Copy",
              "options": {
                "rawHtml": "<p>\n  Once deployed to an environment, clients can interact with on-chain programs\n  through RPC connections to each cluster.\n</p>\n"
              }
            },
            "responsiveStyles": {
              "large": {
                "display": "flex",
                "flexDirection": "column",
                "position": "relative",
                "flexShrink": "0",
                "boxSizing": "border-box",
