          {
            "@type": "@builder.io/sdk:Element",
            "@version": 2,
            "id": "builder-2e7d50f295cd4e2a863bd60d2ccb97f5",
            "meta": {
              "previousId": "builder-04e5dc172fc54b4887d10bbd128eb597"
            },
            "component": {
              "name": "Columns",
              "options": {
