{
  "version": 1.6,
  "locale": {
    "source": "en",
    "targets": ["zh"]
  },
  "buckets": {
    "json": {
      "lockedKeys": ["pages"],
      "include": [
        "content/docs/[locale]/clients/meta.json",
        "content/docs/[locale]/core/meta.json",
        "content/docs/[locale]/intro/meta.json",
        "content/docs/[locale]/intro/quick-start/meta.json",
        "content/docs/[locale]/programs/meta.json",
        "content/docs/[locale]/programs/rust/meta.json",
        "content/docs/[locale]/references/meta.json",
        "content/docs/[locale]/references/feature-gates/meta.json",
        "content/docs/[locale]/references/staking/meta.json",
        "content/docs/[locale]/tokens/meta.json",
        "content/docs/[locale]/tokens/basics/meta.json",
        "content/docs/[locale]/tokens/extensions/meta.json",
        "content/docs/[locale]/tokens/extensions/confidential-transfer/meta.json",
        "content/docs/[locale]/meta.json"
      ]
    },
    "mdx": {
      "include": [
        "content/docs/[locale]/clients/*.mdx",
        "content/docs/[locale]/core/*.mdx",
        "content/docs/[locale]/intro/*.mdx",
        "content/docs/[locale]/intro/quick-start/*.mdx",
        "content/docs/[locale]/programs/*.mdx",
        "content/docs/[locale]/programs/rust/*.mdx",
        "content/docs/[locale]/references/*.mdx",
        "content/docs/[locale]/references/feature-gates/*.mdx",
        "content/docs/[locale]/references/staking/*.mdx",
        "content/docs/[locale]/tokens/*.mdx",
        "content/docs/[locale]/tokens/basics/*.mdx",
        "content/docs/[locale]/tokens/extensions/*.mdx",
        "content/docs/[locale]/tokens/extensions/confidential-transfer/*.mdx",
        "content/docs/[locale]/index.mdx"
      ]
    }
  },
  "$schema": "https://lingo.dev/schema/i18n.json"
}
